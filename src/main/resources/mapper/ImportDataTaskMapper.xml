<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geekq.highimporttry.mapper.ImportDataTaskDao">
    <resultMap id="BaseResultMap" type="com.geekq.highimporttry.entity.ImportDataTask">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="day" jdbcType="VARCHAR" property="day"/>
        <result column="type" jdbcType="VARCHAR" property="type"/>
        <result column="status" jdbcType="INTEGER" property="status"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime"/>
        <result column="version" jdbcType="INTEGER" property="version"/>
    </resultMap>

    <sql id="Base_Column_List">
    id, day, type, status, create_time, update_time, version
    </sql>

    <select id="queryTodayTaskIsImport" resultType="java.lang.Integer">
        select count(*) from import_data_task where day=#{day,jdbcType=VARCHAR}
    </select>

</mapper>